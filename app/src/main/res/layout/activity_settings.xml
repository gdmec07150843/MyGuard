<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:itcast="http://schemas.android.com/tools"
    android:orientation="vertical">

    <cn.edu.gdmec.s07150843.myguard.m10settings.widget.SettingView
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:id="@+id/sv_blacknumber_set"
        itcast:status_on="黑名单拦截已开启"
        app:status_off="黑名单拦截设置已关闭"
        app:settitle="黑名单拦截设置"
        />
    <cn.edu.gdmec.s07150843.myguard.m10settings.widget.SettingView
        android:id="@+id/sv_applock_set"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:status_on="程序锁已开启"
        app:status_off="程序锁已关闭"
        app:settitle="程序锁设置"/>

</LinearLayout>
