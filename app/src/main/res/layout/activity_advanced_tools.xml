<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom_android="http://schemas.android.com/apk/res/cn.edu.gdmec.s07150843.myguard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
<include layout="@layout/titlebar"/>

<include layout="@layout/titlebar"/>

 <cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView
        android:id="@+id/advanceview_numbelongs"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        custom_android:desc="号码归属地查询"
        android:gravity="center_vertical"
        android:src="@drawable/numofbelongto">
    </cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView>

    <cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView
        android:id="@+id/advanceview_smsbackup"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        custom_android:desc="短信备份"
        android:layout_marginTop="3dp"
        android:gravity="center_vertical"
        android:src="@drawable/sms_backup">
    </cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView>


    <cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView
        android:id="@+id/advanceview_smsreducition"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        custom_android:desc="短信还原"
        android:layout_marginTop="3dp"
        android:gravity="center_vertical"
        android:src="@drawable/sms_reduction">
        </cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView>

     <cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView
         android:layout_width="match_parent"
         android:layout_height="55dp"
         android:id="@+id/advanceview_smsbackup"
         custom_android:desc="短信备份"
         android:layout_marginTop="3dp"
         android:src="@drawable/sms_backup">

     </cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView>

    <cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView
        android:id="@+id/advanceview_applock"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        custom_android:desc="程序锁"
        android:layout_marginTop="3dp"
        android:gravity="center_vertical"
        android:src="@drawable/programoflock">



    </cn.edu.gdmec.s07150843.myguard.m9advancedtools.widget.AdvancedToolsView>




</LinearLayout>
